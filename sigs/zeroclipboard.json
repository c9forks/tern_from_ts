{
  "ZeroClipboard": {
    "!type": "fn(elements?: ?, options?: ZeroClipboardOptions)",
    "config": "fn(options: ZeroClipboardOptions)",
    "destroy": "fn()",
    "emit": "fn(eventName: string, args: ?)",
    "prototype": {
      "activate": "fn(element: ?)",
      "setText": "fn(newText: string)",
      "title": "fn(newTitle: string)",
      "setSize": "fn(width: number, height: number)",
      "version": "string",
      "moviePath": "string",
      "trustedDomains": "?",
      "text": "string",
      "hoverClass": "string",
      "activeClass": "string",
      "deactivate": "fn()",
      "ready": "bool",
      "reposition": "fn()",
      "on": "fn(eventName: string, func: Function)",
      "off": "fn(eventName: string, func: Function)",
      "clip": "fn(elements: ?)",
      "unclip": "fn(elements: ?)"
    }
  },
  "ZeroClipboardOptions": {
    "autoActivate": "bool",
    "cacheBust": "bool",
    "debug": "bool",
    "forceHandCursor": "bool",
    "moviePath": "string",
    "swfPath": "string",
    "trustedDomains": "?",
    "title": "string",
    "zIndex": "number",
    "activeClass": "string",
    "hoverClass": "string",
    "allowScriptAccess": "string",
    "trustedOrigins": "?",
    "useNoCache": "bool"
  },
  "zeroclipboard": "ZeroClipboard",
  "!name": "zeroclipboard.d.ts"
}
